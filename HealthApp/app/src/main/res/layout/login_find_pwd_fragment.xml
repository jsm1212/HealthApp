<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".login.FindPwdFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="513dp"
            android:layout_height="400dp"
            android:id="@+id/constraintLayout5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        <TextView
                android:text="아이디"
                android:textSize="20dp"
                android:gravity="center"
                android:layout_width="136dp"
                android:layout_height="56dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.146" app:layout_constraintVertical_bias="0.169"
                android:id="@+id/findIdText2"/>
        <TextView
                android:textSize="20dp"
                android:gravity="center"
                android:text="이름"
                android:layout_width="136dp"
                android:layout_height="56dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.145" app:layout_constraintVertical_bias="0.473"
                android:id="@+id/findNameText2"/>
        <TextView
                android:textSize="20dp"
                android:gravity="center"
                android:text="전화번호"
                android:layout_width="136dp"
                android:layout_height="56dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.145" app:layout_constraintVertical_bias="0.791"
                android:id="@+id/findTelText2"/>
        <EditText
                android:textSize="18dp"
                android:gravity="left"
                android:layout_width="251dp"
                android:layout_height="62dp"
                android:inputType="textPersonName"
                android:ems="10"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.846" app:layout_constraintVertical_bias="0.173"
                android:id="@+id/findIdEditText2"/>
        <EditText
                android:textSize="18dp"
                android:gravity="left"
                android:layout_width="251dp"
                android:layout_height="62dp"
                android:inputType="textPersonName"
                android:ems="10"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.843" app:layout_constraintVertical_bias="0.482"
                android:id="@+id/findNameEditText2"/>
        <EditText
                android:textSize="18dp"
                android:gravity="left"
                android:layout_width="251dp"
                android:layout_height="62dp"
                android:inputType="textPersonName"
                android:ems="10"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHorizontal_bias="0.846" app:layout_constraintVertical_bias="0.782"
                android:id="@+id/findTelEditText2"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
            android:textSize="20dp"
            android:gravity="center"
            android:text="비밀번호 찾기"
            android:layout_width="209dp"
            android:layout_height="63dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintVertical_bias="0.238" android:id="@+id/findPwdText"/>
    <Button
            android:textSize="18dp"
            android:gravity="center"
            android:text="찾기"
            android:layout_width="256dp"
            android:layout_height="74dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.803" android:id="@+id/findMyPwdBtn"/>

</androidx.constraintlayout.widget.ConstraintLayout>