<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/bbsback"
        tools:context=".mypage.MypageRoutineActivity">
    <TextView
            android:id="@+id/textrt"
            android:text="운동루틴목록"
            android:gravity="center" android:textSize="20sp"
            android:layout_width="match_parent" android:layout_height="50dp"/>
    <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyRoutine"
            android:layout_width="match_parent"
            android:layout_height="800dp"
            app:layout_constraintTop_toBottomOf="@+id/textrt"/>
</androidx.constraintlayout.widget.ConstraintLayout>