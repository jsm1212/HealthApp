<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".login.FindActivity">
    <fragment android:layout_width="match_parent"
              android:layout_height="60dp"
              android:id="@+id/find_fragment_menu"
              android:name="com.example.healthapp.login.FindFragmentMenu"
              tools:layout="@layout/login_find_fragment_menu"
              tools:ignore="Instantiatable,MissingConstraints"/>
    <FrameLayout
            android:id="@+id/content"
            android:layout_width="540dp"
            android:layout_height="830dp"
            app:layout_constraintTop_toTopOf="@+id/find_fragment_menu"
            android:layout_marginTop="24dp" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>